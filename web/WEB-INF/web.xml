<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login1</servlet-name>
        <servlet-class>loginView</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logoutView</servlet-name>
        <servlet-class>logoutView</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editView</servlet-name>
        <servlet-class>editView</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>listView</servlet-name>
        <servlet-class>listView</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>depositView</servlet-name>
        <servlet-class>depositView</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>withdrowView</servlet-name>
        <servlet-class>withdrowView</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login1</servlet-name>
        <url-pattern>/login1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logoutView</servlet-name>
        <url-pattern>/logoutView</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editView</servlet-name>
        <url-pattern>/editView</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>listView</servlet-name>
        <url-pattern>/listView</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>depositView</servlet-name>
        <url-pattern>/depositView</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>withdrowView</servlet-name>
        <url-pattern>/withdrowView</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
